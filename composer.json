{
    "name": "dzangolab/laravel-api-auth",
    "description": "Authentication and authorization service for laravel api projects.",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "authors": [
        {
            "name": "uddhab",
            "email": "uddhab@dzangolab.com"
        }
    ],
    "license": "proprietary",
    "require": {
        "ext-openssl": "*",
        "ext-json": "*",
        "illuminate/auth": "^7.4.0",
        "illuminate/validation": "^7.4.0",
        "laravel/passport": "^8.4.3"
    },
    "autoload": {
        "psr-4": {
            "Dzangolab\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dzangolab\\Auth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Dzangolab\\Auth\\AuthServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
